package problem15;


/**
 * @author Cliff_Ford
 * 输入一个链表，反转链表后，输出新链表的表头。
 */
public class Solution {
	public ListNode ReverseList(ListNode head) {
		//空链表
		if(head == null) {
			return null;
		}
		//单个节点链表
		if(head.next == null) {
			return head;
		}
		//两个以上
		ListNode p = head;
		ListNode pL = p;
		ListNode q = p.next;
		while(q.next != null) {
			pL = q;
			q = q.next;
		}
		pL.next = null;
		ListNode t = ReverseList(p);
		q.next = t;
		return q;
    }
	public void Reverse(ListNode head) {
		if(head.next != null) {
			Reverse(head.next);
		}
		System.out.println(head.val);
	}
}