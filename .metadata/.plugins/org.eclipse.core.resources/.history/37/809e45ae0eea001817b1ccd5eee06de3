	    	   return list;
	       }
	       if(str.length() == 1) {
	    	   list.add(str);
	    	   return list;
	       }
	       int len = str.length();
	       // if(len>1)
	       Permutation(str.substring(0, len));
	       String last = String.valueOf(str.charAt(len-1));
