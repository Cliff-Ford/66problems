package problem16;


/**
 * @author Cliff_Ford
 * 输入两个单调递增的链表，输出两个链表合成后的链表，
 * 当然我们需要合成后的链表满足单调不减规则。
 */
public class Solution {
	public ListNode Merge(ListNode list1,ListNode list2) {
		if(list1 == null) {
			return list2;
		}
		if(list2 == null) {
			return list1;
		}
		ListNode h = null;
        ListNode p = null;
        ListNode q = null;
        if(list1.val <= list2.val) {
        	p = list1;
        	q = list2;
        	h = p;
        }else {
        	p = list2;
        	q = list1;
        	h = p;
        }
        while(result != null && p != null) {
        	if(result.val <= p.val) {
        		
        	}
        }
        return result;
    }
}