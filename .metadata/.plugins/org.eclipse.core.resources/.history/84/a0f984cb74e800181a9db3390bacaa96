package problem15;


/**
 * @author Cliff_Ford
 * 输入一个链表，反转链表后，输出新链表的表头。
 */
public class Solution {
	public ListNode ReverseList(ListNode head) {
		ListNode p,t;
		p = head;
		t = head;
		while(t.next != null) {
			t = t.next;
		}
		while(p!=t) {
			int v = p.val;
			p.val = t.val;
			t.val = v;
			p = p.next;
			//出错了
			t = t.next;
		}
		return head;
    }
	public void Reverse(ListNode head) {
		if(head.next != null) {
			Reverse(head.next);
		}
		System.out.println(head.val);
	}
}